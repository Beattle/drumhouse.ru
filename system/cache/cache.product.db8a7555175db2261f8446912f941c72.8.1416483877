	&lt;li&gt;
		&lt;font color=&quot;#333333&quot; face=&quot;Arial, Helvetica, sans-serif&quot; size=&quot;1&quot;&gt;&lt;font color=&quot;#333333&quot; face=&quot;Arial, Helvetica, sans-serif&quot; size=&quot;1&quot;&gt;White Marine Pearl(WMP)&lt;/font&gt;&lt;/font&gt;&lt;/li&gt;
	&lt;li&gt;
		&lt;font color=&quot;#333333&quot; face=&quot;Arial, Helvetica, sans-serif&quot; size=&quot;1&quot;&gt;&lt;font color=&quot;#333333&quot; face=&quot;Arial, Helvetica, sans-serif&quot; size=&quot;1&quot;&gt;Black Satin (BKS)&lt;/font&gt;&lt;/font&gt;&lt;/li&gt;
	&lt;li&gt;
